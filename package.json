{
  "name": "frontier",
  "version": "1.0.0",
  "main": "./src/server/index.cjs",
  "types": "./src/ui/main.d.ts",
  "scripts": {
    "js": "npx webpack",
    "css": "npx @tailwindcss/cli -o ./public/main.css",
    "ui": "npx webpack && npx @tailwindcss/cli -o ./public/main.css && cp src/ui/index.html public/",
    "migrate": "npx prisma migrate dev --name init --schema=./src/server/prisma/schema.prisma && npx prisma generate --schema=./src/server/prisma/schema.prisma",
    "start": "node ./src/server/index.cjs"
  },
  "keywords": [
    "Fastify",
    "Typescript",
    "Tailwindcss",
    "SQLite"
  ],
  "author": "",
  "license": "ISC",
  "description": "frontier project",
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/jwt": "^9.1.0",
    "@fastify/static": "^8.2.0",
    "@fastify/websocket": "^11.0.2",
    "@prisma/client": "^6.8.2",
    "bcrypt": "^6.0.0",
    "dotenv": "^16.5.0",
    "fastify": "^5.3.3",
    "google-auth-library": "^9.15.1",
    "qrcode": "^1.5.4",
    "speakeasy": "^2.0.0"
  },
  "devDependencies": {
    "@prisma/client": "6.8.2",
    "@tailwindcss/cli": "^4.1.7",
    "pino-pretty": "^13.0.0",
    "prisma": "^6.8.2",
    "tailwindcss": "^4.1.7",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  }
}
